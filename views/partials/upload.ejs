<form id="upload-form" action="/uploads" enctype="multipart/form-data" method="post">
    <label for="title">Filename:</label>
    <input id="title"type="text" class="form-control" name="title" placeholder="Enter a name for your file." />
    <input type="file" class="form-control-file" name="files" >
    <input id="upload-btn" type="submit"  class="btn btn-dark" value="Upload">
</form>